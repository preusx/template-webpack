  "scripts": {
    "gulp:watch": "gulp --gulpfile build/gulp/base.js --cwd ./ watch",
    "gulp:build": "gulp --gulpfile build/gulp/base.js --cwd ./ build",
    "wp:dev": "cross-env NODE_ENV=development webpack --config build/webpack/dev.js --progress --colors --watch",
    "wp:watch": "npm run wp:dev",
    "wp:server": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack/server.js --open --inline --hot",
    "wp:build": "cross-env NODE_ENV=production webpack --config build/webpack/dist.js",
    "watch": "./node_modules/.bin/concurrently \"npm run gulp:watch\" \"npm run wp:watch\"",
    "build": "./node_modules/.bin/concurrently \"npm run gulp:build\" \"npm run wp:build\"",
    "start": "npm run watch",
    "dev": "npm run watch"
  },
